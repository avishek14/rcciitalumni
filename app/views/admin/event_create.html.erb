
<%= form_tag admin_create_event_path do %>
	<%= label_tag :date_on, "Event Date:" %>
	<%= date_select :don, :d %>
	<br/>
	<%= label_tag :title, "Event Title:" %>
	<%= text_field_tag :title %>
	<br/>
	<%= label_tag :desc, "Event Description:" %><br/>
	<%= textile_editor_tag :desc, nil, size: "123x17" %>
	<br/>
	<%= textile_editor_initialize %> 
	<%= submit_tag "Create", id: :create %>
<% end %>