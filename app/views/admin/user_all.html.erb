<div class = 'admin-user-all-wrapper'>
	<%= paginate @u_all %>
	<ul>
	<% @u_all.each do |u| %>
		<%= link_to user_page_path id: u.id do %><li>
			<% if u.active %>
				<%= image_tag u.image.to_s, size: "100x100" %>
			<% else %>
				<%= image_tag u.image.to_s, size: "100x100", style: "border-color: black" %>
			<% end %>
			<h3><%= "#{u.fname} #{u.lname}" %></h3>
		</li><% end %>
	<% end %>
	</ul>
</div>